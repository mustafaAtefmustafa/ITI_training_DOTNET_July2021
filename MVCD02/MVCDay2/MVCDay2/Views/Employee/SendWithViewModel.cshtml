@model List<MVCDay2.Models.Employee>
@{
    ViewBag.Title = "SendWithViewModel";
    Layout = "~/Views/Shared/_MyLayout.cshtml";

}

<h2>SendWithViewModel</h2>
<table class="table table-hover table-dark">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Address</th>
        <th>Age</th>
        <th>Salary</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ID</td>
            <td>@item.Name</td>
            <td>@item.Address</td>
            <td>@item.Age</td>
            <td>@item.Salary</td>

        </tr>
    }
</table>

